            </div>
            <div class="flex gap-8 items-center">
                <a href="https://breakthru.ai/" class="text-slate-600 hover:text-blue-600 font-medium">Main Website</a>
                <a href="https://breakthru.ai/" class="bg-slate-900 text-white px-6 py-2 rounded text-sm hover:bg-slate-800 transition-colors">Request Deep Dive</a>
            </div>
        </div>
    </footer>

    <script>
        // Data Input Object
        const companyData = {
  "companyIdentity": {
    "name": "",
    "domain": "",
    "countryOrRegion": "",
    "companyType": "",
    "CompanyLogoURL": "",
    "confidence": "low"
  },
  "businessOverview": {
    "whatTheyDo": "",
    "productsOrServices": [],
    "businessModel": "",
    "confidence": "low"
  },
  "industryFocus": {
    "primaryIndustry": "",
    "subIndustry": "",
    "operationalFocusAreas": [],
    "confidence": "low"
  },
  "targetAudience": {
    "customerType": "",
    "industriesServed": [],
    "customerSize": "",
    "confidence": "low"
  },
  "technologyAndOperations": {
    "mentionedTechnologies": [],
    "digitalMaturity": "",
    "operationalComplexityIndicators": [],
    "confidence": "low"
  },
  "costOptimizationSignals": {
    "likelyHighCostAreas": [],
    "impliedBusinessChallenges": [],
    "confidence": "low"
  },
  "brandAndMessaging": {
    "brandTone": "",
    "websiteKeywords": [],
    "confidence": "low"
  },
  "dataLineage": {
    "agentCount": 0,
    "fieldsConfirmedByMultipleAgents": [],
    "fieldsWithConflicts": [],
    "fieldsPrimarilyInferred": []
  }
};

        $(document).ready(function() {
            // Populate Name
            const name = companyData.companyIdentity.name || "Your Organization";
            $('#company-name-hero, #company-name-personal, #footer-company-name').text(name);

            // Populate Logo
            if(companyData.companyIdentity.CompanyLogoURL) {
                $('#customer-logo').attr('src', companyData.companyIdentity.CompanyLogoURL).removeClass('hidden');
            }

            // Populate Industry/Context
            if(companyData.industryFocus.primaryIndustry) {
                $('#industry-display').text(companyData.industryFocus.primaryIndustry);
                $('#personalization-text').html(`For an organization in the <span class="font-semibold text-blue-600">${companyData.industryFocus.primaryIndustry}</span> sector, the stakes of digital transformation extend beyond efficiency—they are about competitive resilience. By addressing your unique operational complexity, Breakthru AI Solutions positions itself as a strategic partner to help you pivot from reactive maintenance to proactive innovation.`);
            }

            // Populate About text
            if(companyData.businessOverview.whatTheyDo) {
                $('#company-about').text(companyData.businessOverview.whatTheyDo);
            }

            // Populate Complexity/Scale
            if(companyData.companyIdentity.countryOrRegion) {
                $('#scale-display').text(companyData.companyIdentity.countryOrRegion + " Operations");
            }

            // Hero Subheadline Update
            if(companyData.industryFocus.primaryIndustry) {
                $('#hero-subheadline').text(`Transforming ${companyData.industryFocus.primaryIndustry} through intelligent operations, cloud optimization, and data-driven agility.`);
            }
        });
